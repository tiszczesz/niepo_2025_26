@model User
<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>

<body>
    <form method="post" asp-action="Create">
        <div>
            <label asp-for="UserName"></label>
            <input type="text" asp-for="UserName" />
